{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/submit-whatsapp",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/submit-booking",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/submit-contact",
      "methods": ["POST"],
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
